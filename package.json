{
  "name": "psychology-website",
  "private": true,
  "version": "1.0.0",
  "description": "Website for psychology business created by @TomSssM",
  "scripts": {
    "dev": "npm-run-all --parallel watch server",
    "build": "npm-run-all build:*",
    "build:js": "babel src --out-dir build",
    "build:scss": "sass src:build --no-source-map",
    "build:css": "postcss build/styles.css --replace --no-map",
    "watch": "npm-run-all --parallel watch:*",
    "watch:js": "npm run build:js -- --watch",
    "watch:scss": "sass src:build --watch",
    "server": "live-server",
    "clean": "rimraf build",
    "prebuild": "npm run clean",
    "postinstall": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TomSssM/psychology-website.git"
  },
  "devDependencies": {
    "@babel/cli": "7.21.0",
    "@babel/core": "7.21.4",
    "@babel/preset-env": "7.21.4",
    "autoprefixer": "10.4.14",
    "live-server": "1.2.2",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.23",
    "postcss-cli": "10.1.0",
    "rimraf": "5.0.0",
    "sass": "1.62.0"
  },
  "engines": {
    "node": ">=16.16.0",
    "npm": ">=8.11.0"
  },
  "keywords": [
    "psychology",
    "website"
  ],
  "author": "TomSssM",
  "license": "ISC"
}
